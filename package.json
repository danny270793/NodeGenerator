{
    "name": "@danny270793/nodegenerator",
    "description": "Generates a node base scaffold project",
    "version": "1.0.2",
    "files": [
        "./build/**/*",
        "./assets/**/*"
    ],
    "main": "./build/index.js",
    "bin": "./build/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node ./build/index.js",
        "start:watch": "ts-node-dev --respawn ./src/index.ts",
        "lint": "eslint --fix .",
        "format": "prettier --write .",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/danny270793/nodegenerator"
    },
    "keywords": [
        "typescript", "eslint", "prettier", "jest", "ts-node-dev"
    ],
    "author": {
        "email": "danny270793@icloud.com",
        "name": "Danny Vaca",
        "url": "https://danny270793.github.io"
    },
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@types/jest": "^29.5.14",
        "eslint": "^9.17.0",
        "globals": "^15.14.0",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "ts-jest": "^29.2.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.19.0"
    }
}
